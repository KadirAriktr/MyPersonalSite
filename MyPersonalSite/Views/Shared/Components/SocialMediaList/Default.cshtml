@using EntityLayer.Concrete
@model List<SocialMedia>


<div class="social-nav" data-aos="fade-up" data-aos-delay="200">
    <nav role="navigation">
        <ul class="nav justify-content-left">
          
            @foreach(var items in Model)
            {
                <li class="nav-item"><a class="nav-link" href="@items.Url" title="@items.SocialMediaName"><i class="@items.Icon"></i><span class="menu-title sr-only">@items.SocialMediaName</span></a></li>
            }
           
        </ul>
    </nav>
</div>